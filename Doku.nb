Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"f", "[", "n", "]"}], "=", " ", 
   RowBox[{
    RowBox[{
     UnderoverscriptBox["\[Sum]", 
      RowBox[{"m", " ", "=", " ", "0"}], 
      RowBox[{
       RowBox[{"N", " ", "/", " ", "2"}], "-", " ", "1"}]], 
     RowBox[{"(", 
      RowBox[{
       SubscriptBox["c", 
        RowBox[{"2", "m"}]], 
