        RowBox[{"N", " ", "/", " ", "2"}], "-", " ", "1"}]], 
      RowBox[{"(", 
       RowBox[{
        SubscriptBox["c", 
         RowBox[{"(", 
          RowBox[{
           RowBox[{"2", "m"}], "+", "1"}], ")"}]], 
        SuperscriptBox["e", 
         RowBox[{
          RowBox[{
           RowBox[{"-", "j"}], 
           FractionBox[
            RowBox[{"2", "\[Pi]"}], 
            RowBox[{"N", "/", "2"}]], "m", " ", "n"}], ")"}]]}], ")"}], 
      " "}]}]}]}], TraditionalForm]], "EquationNumbered",
 CellChangeTimes->{{3.5801438015913258`*^9, 3.580143823826132*^9}, {
